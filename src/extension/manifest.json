{
    "name": "BBB Playbackspeed",
    "description": "Adds the capability to change playback speed when watching a BBB recording",
    "version": "1.0",
    "author": "Andreas Hartung",
    "permissions": [
      "activeTab",
      "debugger",
      "https://teach.informatik.tu-freiberg.de/"
    ],
    "web_accessible_resources": [
        "icon.svg"
    ],
    "content_scripts": [
      {
        "matches": ["*://teach.informatik.tu-freiberg.de/*"],
        "run_at": "document_end",
        "js": [
            "content-script.js",
            "jquery.min.js",
            "popcorn-complete.min.js"
        ]
      }
    ],
    "manifest_version": 2
  }